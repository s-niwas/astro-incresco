---
import ContentSection from "~/components/home/ContentSection.astro";
import TechnologyList from "~/components/home/TechnologyList.astro";

const { technologySection } = Astro.props;
const { techTopSection, techStacks } = technologySection[0];
---

<ContentSection title={techTopSection[0]?.title} id="technology">
  <Fragment slot="lead">
    {techTopSection[0]?.subTitle}
  </Fragment>
  <TechnologyList items={techStacks} />
</ContentSection>
